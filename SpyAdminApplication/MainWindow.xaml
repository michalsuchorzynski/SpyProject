<Window x:Class="SpyAdminApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpyAdminApplication"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="500">

    <DockPanel Name="DockPanelMain">
        <Menu DockPanel.Dock="Top">
            <MenuItem Name="MenuItemStart" Header="Start" Click="MenuItemStart_Click"></MenuItem>
            <MenuItem Name="MenuItemPages" Header="Akpectowalne strony" Click="MenuItemPages_Click"></MenuItem>
        </Menu>
        <Grid>
            <Frame x:Name="firstFrame" NavigationUIVisibility="Hidden"></Frame>
        </Grid>

    </DockPanel>

</Window>
